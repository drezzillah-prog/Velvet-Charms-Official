{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "data/**/*", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "vercel.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/data/(.*)", "dest": "/data/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
