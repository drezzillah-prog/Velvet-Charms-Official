{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "/*.html", "use": "@vercel/static" },
    { "src": "/style.css", "use": "@vercel/static" },
    { "src": "/script.js", "use": "@vercel/static" },
    { "src": "/catalogue.json", "use": "@vercel/static" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
